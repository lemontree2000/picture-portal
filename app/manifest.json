{
  "name": "Picture Portal",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "stacksweet.com Chrome Extension",
  "author": "Edward <gonelove.guo@gmail.com>",
  "homepage_url": "https://github.com/lemontree2000",
  "icons": {
    "16": "icons/portal.png",
    "128": "icons/portal.png"
  },
  "background": {
    "scripts": ["scripts/chromereload.js", "scripts/background.js"]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "background",
    "storage",
    "alarms",
    "webNavigation",
    "notifications"
  ],
  "options_ui": {
    "page": "ui/index.html#option",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["http://local.baozun.com/*"],
      "js": ["scripts/content.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/portal.png",
      "38": "icons/portal.png"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": []
}
